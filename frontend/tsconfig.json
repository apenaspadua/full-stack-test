{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "application/*": ["src/application/*"],
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "core/*": ["src/core/*"],
      "navigation/*": ["src/navigation/*"],
      "presentation/*": ["src/presentation/*"],
      "types/*": ["src/types/*"],
    }
  }
}
